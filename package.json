{
  "name": "node-laravel-framework",
  "version": "1.0.0",
  "private": true,
  "description": "Node Laravel Style Framework",
  "author": "trexwb",
  "type": "module",
  "main": "./dist/src/public/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/public/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/src/public/index.js",
    "artisan": "node ./dist/src/artisan.js",
    "artisan:dev": "ts-node -r tsconfig-paths/register src/artisan.ts",
    "knex": "knex --knexfile src/database/knexfile.ts",
    "migrate:make": "npm run knex migrate:make -- -x ts",
    "migrate:latest": "npm run knex migrate:latest",
    "seed:make": "npm run knex seed:make -- -x ts",
    "seed:run": "npm run knex seed:run",
    "postinstall": "node ./install.js"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "knex": "^3.1.0",
    "lodash-es": "^4.17.23",
    "multer": "^2.0.2",
    "mysql2": "^3.16.2",
    "node-schedule": "^2.1.1",
    "objection": "^3.1.5",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "sqlite3": "^5.1.7",
    "validatorjs": "^3.22.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10",
    "@types/node-schedule": "^2.1.8",
    "@types/validatorjs": "^3.15.5",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "imports": {
    "#app/*": "./src/app/*.ts",
    "#bootstrap/*": "./src/bootstrap/*.ts",
    "#config/*": "./src/config/*.ts",
    "#database/*": "./src/database/*.ts",
    "#public/*": "./src/public/*.ts",
    "#resources/*": "./src/resources/*.ts",
    "#routes/*": "./src/routes/*.ts",
    "#storage/*": "./src/storage/*.ts",
    "#utils/*": "./src/utils/*.ts"
  }
}